<form method="post" action="{{ path('app_back_garden_deletePicture', {'id': picture.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer l\'image?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
    <button class="btn btn-danger btn-sm my-2">Supprimer</button>
</form>