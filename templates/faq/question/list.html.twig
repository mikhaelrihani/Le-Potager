{% extends 'faq/base.html.twig' %}

{% block title %}Questions posées
{% endblock %}

{% block body %}
	<h1 class="text-center ">Questions posées</h1>
	<p class="tags text-center">Filtrer par mot-clé :
		{% for tag in tags %}
			{% include 'faq/question/_tags.html.twig' %}
		{% endfor %}
	</p>


	{% for question in questions %}
		<hr class="my-4">
		{% include 'faq/question/_detail.html.twig' %}
	{% endfor %}

{% endblock %}
